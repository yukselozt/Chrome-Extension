{
  "name": "Kuram",
  "version": "1.0",
  "manifest_version": 2,
  "background": {
    "scripts": ["./Kuram.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": { "16": "icon.png" },
  "permissions": ["tabs", "*://*/*"],
  "web_accessible_resources": ["js/*", "html/*", "css/*", "img/*"]
}
